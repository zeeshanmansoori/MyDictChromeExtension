{
"manifest_version": 3,
"name": "MyDict",
"version": "1.0",
"description": "Personal dictionary",
"icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128":"images/icon.png"
   },
   "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
"background": {
    "service_worker": "js/background.js"
},
"permissions": ["storage"]

}